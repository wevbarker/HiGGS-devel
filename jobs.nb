            RowBox[{
             RowBox[{"{", 
              RowBox[{
               RowBox[{"xAct`HiGGS`$IfConstraints", "[", 
                RowBox[{"[", "ii", "]"}], "]"}], ",", 
               RowBox[{"IndIfConstraints", "[", 
                RowBox[{"[", "jj", "]"}], "]"}], ",", 
               RowBox[{"xAct`HiGGS`PoissonBracket", "[", 
                RowBox[{
                 RowBox[{"xAct`HiGGS`$IfConstraints", "[", 
                  RowBox[{"[", "ii", "]"}], "]"}], ",", 
                 RowBox[{"IndIfConstraints", "[", 
                  RowBox[{"[", "jj", "]"}], "]"}]}], "]"}]}], "}"}], ",", 
             RowBox[{"{", 
              RowBox[{"ii", ",", 
               RowBox[{"Length", "@", "xAct`HiGGS`$IfConstraints"}]}], "}"}], 
